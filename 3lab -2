using System;
using System.Collections;
namespace Lab3
{
    class Program
    {
        static void Main(string[] args)
        {
            #region Task6_L1
            static void L1N6()
            {
               // int n = 5;
                double[] a = new double[5] { 1, 1, 1, 1, 1 };
                Console.WriteLine("Введите числа x1..x5 через пробел\nИзначально каждому x присвоено значение 1");
                string s = Console.ReadLine();
                string[] c = s.Split(' ');
                double sum = 0;

                for (int i = 0; i < 5; i++)
                {
                    if ((c.Length < 5) || (c.Length > 5))
                    {
                        Console.WriteLine("Длина массива не может быть меньше или больше 5");
                        break;
                    }
                    bool norm = double.TryParse(c[i], out a[i]);
                    if (!norm)
                    {
                        Console.WriteLine("Вы ввели что-то не так");
                    }
                    sum += a[i] * a[i];
                }
                
                Console.WriteLine($"L = {Math.Sqrt(sum)}");
            }
            #endregion
            #region Task10_L1
            static void L1N10()
            {
                double[] a = new double[10];

                Console.WriteLine("Введите 10 чисел через пробел");
                string s = Console.ReadLine();
                string[] c = s.Split(' ');
                double P, Q;
                Console.WriteLine("Введите число P");
                bool normA = double.TryParse(Console.ReadLine(), out P);
                Console.WriteLine("Введите число Q");
                bool normB = double.TryParse(Console.ReadLine(), out Q);
                bool normik = normA & normB;

                int endik = 0;
                for (int i = 0; i < 10; i++)
                {

                    if ((c.Length < 10) || (c.Length > 10))
                    {
                        Console.WriteLine("Длина массива не может быть меньше или больше 10");
                        break;
                    }
                    if (!normik || P >= Q)
                    {
                        Console.WriteLine("Значения P и Q введены неверно");
                        break;
                    }
                    bool norm = double.TryParse(c[i], out a[i]);
                    if (!norm)
                    {
                        Console.WriteLine("Вы ввели что-то не так");
                    }
                    if ((a[i] > P) & (a[i] < Q))
                    {
                        endik += 1;
                    }
                }
                
                
                Console.WriteLine($"\nКол-во точек между P и Q: {endik}");
            }
            #endregion
            #region Task11_L1
            static void L1N11()
            {
                double[] a = new double[10];
                Console.WriteLine("Введите 10 чисел через пробел");
                string s = Console.ReadLine();
                string[] c = s.Split(' ');
                double[] end = new double[10];
                int countik = 0;

                for (int i = 0; i < 10; i++)
                {
                    if ((c.Length < 10) || (c.Length > 10))
                    {
                        Console.WriteLine("Длина массива не может быть меньше или больше 10");
                        break;
                    }
                    bool norm = double.TryParse(c[i], out a[i]);
                    if (!norm)
                    {
                        Console.WriteLine("Вы ввели что-то не так");
                    }
                    if (a[i] > 0)
                    {
                        end[countik] = a[i];
                        countik += 1;
                    }
                }
                
                double[] endik = new double[countik];
                for (int i = 0; i < countik; i++)
                {
                    endik[i] = end[i];
                    Console.Write($"{endik[i]} ");
                }

            }
