            #region Task1_L3
            static void L3N1()
            {

                int ammount;
                Console.WriteLine("Введите кол-во элементов будущего массива");
                bool normul = int.TryParse(Console.ReadLine(), out ammount);
                if (ammount < 2)
                {
                    Console.WriteLine("Слишком мало");
                    return;
                }
                if (!normul)
                {
                    Console.WriteLine("Вы ввели что-то не то");
                    return;
                }
                double[] a = new double[ammount];
                Console.WriteLine($"Введите {ammount} значений/я массива через пробел ");
                string s = Console.ReadLine();
                string[] c = s.Split(' ');
                int countik = 0;
                double max = double.MinValue;
                double[] positFinal = new double[ammount];
                ArrayList arr = new ArrayList();
                int counter = 0;
                for (int i = 0; i < ammount; i++)
                {
                    if ((c.Length < ammount) || (c.Length > ammount))
                    {
                        Console.WriteLine($"Длина массива не может быть меньше или больше {ammount} ");
                        break;
                    }
                    bool norm = double.TryParse(c[i], out a[i]);
                    if (!norm)
                    {
                        Console.WriteLine("Вы ввели что-то не так");
                    }
                    if (a[i] > max)
                    {

                        max = a[i];
                        countik += 1;
                        arr.Clear();
                    }
                    if (a[i] == max)
                    {
                        arr.Add(i);
                        countik += 1;
                    }

                }

                
                
                if (countik < 2)
                {
                    Console.WriteLine("У макс нет повторений");
                    return;
                }
                double[] Final = new double[counter];

                Console.Write("Новый массив[");
                for (int i = 0; i < arr.Count; i++)
                {
                    Console.Write($"{arr[i]} ");
                }
                Console.WriteLine("]");
            }
            #endregion
            #region Task5_L3
            static void L3N5()
            {

                int ammount;
                Console.WriteLine("Введите кол-во элементов будущего массива");
                bool normul = int.TryParse(Console.ReadLine(), out ammount);
                if (ammount < 2)
                {
                    Console.WriteLine("Слишком мало");
                    return;
                }
                if (!normul)
                {
                    Console.WriteLine("Вы ввели что-то не то");
                    return;
                }
                double[] a = new double[ammount];
                Console.WriteLine($"Введите {ammount} значений/я массива через пробел ");
                string s = Console.ReadLine();
                string[] c = s.Split(' ');
                for (int i = 0; i < ammount; i++)
                {
                    if ((c.Length < ammount) || (c.Length > ammount))
                    {
                        Console.WriteLine($"Длина массива не может быть меньше или больше {ammount} ");
                        break;
                    }
                    bool norm = double.TryParse(c[i], out a[i]);
                    if (!norm)
                    {
                        Console.WriteLine("Вы ввели что-то не так");
                    }
                }
                double[] mass = { };
                if (a.Length % 2 == 0)
                {
                    mass = new double[a.Length / 2];
                }
                else
                {
                    mass = new double[(a.Length + 1) / 2];
                }
                int counter = 0;
                for (int i = 0; i < a.Length; i += 2)
                {
                    mass[counter] = a[i];
                    counter += 1;
                }
                double q, w, e;
                for (q = 0; q < mass.Length; q++)
                {
                    for (int i = 0; i < mass.Length - 1; i++)
                    {
                        if (mass[i] > mass[i + 1])
                        {
                            w = mass[i];
                            mass[i] = mass[i + 1];
                            mass[i + 1] = w;
                        }
                    }
                }

                int cop = 0;
                for (int i = 0; i < a.Length; i++)
                {
                    
                    if (i%2 == 0)
                    {
                        a[i] = mass[cop];
                        cop += 1;
                    }

                    
                    Console.Write($"{a[i]} ");
                }
            }
