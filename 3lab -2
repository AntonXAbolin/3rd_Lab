            #region Task5_L2
            static void L2N5()
            {
                int ammount;
                Console.WriteLine("Введите кол-во элементов будущего массива");
                bool normul = int.TryParse(Console.ReadLine(), out ammount);
                if (ammount < 2)
                {
                    Console.WriteLine("Слишком мало");
                    return;
                }
                if (!normul)
                {
                    Console.WriteLine("Вы ввели что-то не то");
                    return;
                }
                double[] a = new double[ammount];
                Console.WriteLine($"Введите {ammount} значений/я массива через пробел ");
                string s = Console.ReadLine();
                string[] c = s.Split(' ');
                double max = double.MinValue;
                double min = double.MaxValue;
                int posMax = 0;
                int posMin = 0;

                for (int i = 0; i < ammount; i++)
                {
                    if ((c.Length < ammount) || (c.Length > ammount))
                    {
                        Console.WriteLine($"Длина массива не может быть меньше или больше {ammount}");
                        break;
                    }
                    bool norm = double.TryParse(c[i], out a[i]);
                    if (!norm)
                    {
                        Console.WriteLine("Вы ввели что-то не так");
                    }
                    if (a[i] > max)
                    {
                        max = a[i];
                        posMax = i;
                    }
                    if (a[i] < min)
                    {
                        min = a[i];
                        posMin = i;
                    }
                }
                int counter = 0;
                double[] prom = new double[ammount];
                if (posMax < posMin)
                {

                    for (int i = posMax + 1; i < posMin; i++)
                    {

                        if (a[i] < 0)
                        {
                            prom[counter] = a[i];
                            counter += 1;
                        }
                    }
                }
                if (posMax > posMin)
                {
                    for (int i = posMin + 1; i < posMax; i++)
                    {

                        if (a[i] < 0)
                        {
                            prom[counter] = a[i];
                            counter += 1;
                        }
                    }
                }
                double[] arrMin = new double[counter];
                Console.Write("Отрицательный массив: [");
                for (int i = 0; i < arrMin.Length; i++)
                {
                    arrMin[i] = prom[i];
                    Console.Write($"{arrMin[i]} ");
                }

                Console.Write("]");
            }
            #endregion
            #region Task6_L2
            static void L2N6()
            {

                static void Insert(ref double[] arr, double value, int index) // на вход получаем массив, значение, которое надо впихнуть и индекс (на какое место)
                {
                    double[] newArray = new double[arr.Length + 1];

                    /*newArray[index] = value; // Сначала в пустой массив на нужное нам место пихаем значение
                    for (int i = 0; i < index; i++)
                    {
                        newArray[i] = arr[i]; // в этом цикле заполняем пустые места до впихнутого элемента
                    }
                    for (int i = index; i < arr.Length; i++)
                    {
                        newArray[i + 1] = arr[i]; // тут впихиваем оставшееся от старого массива
                    }
                    arr = newArray; // Мы делаем вид, что это все тот же старый массив, хоть и создали новый
                    */
                    int k = index;
                    for (int i = 0; i < newArray.Length; i++)
                    {
                        if (i < index)
                        {
                            newArray[i] = arr[i];
                        }
                        if (i == index)
                        {
                            newArray[index] = value;
                        }
                        if (k < arr.Length)
                        {
                            newArray[k + 1] = arr[k];
                            k++;
                        }
                    }
                    arr = newArray;

                }
                int P;
                Console.WriteLine("Введите P");
                bool normulik = int.TryParse(Console.ReadLine(), out P);
                if (!normulik)
                {
                    Console.WriteLine("Вы ввели что-то не то");
                    return;
                }
                int amount;
                Console.WriteLine("Введите кол-во элементов будущего массива");
                bool normul = int.TryParse(Console.ReadLine(), out amount);
                if (amount < 2)
                {
                    Console.WriteLine("Слишком мало");
                    return;
                }
                if (!normul)
                {
                    Console.WriteLine("Вы ввели что-то не то");
                    return;
                }
                double[] a = new double[amount];
                Console.WriteLine($"Введите {amount} значений/я массива через пробел ");
                string s = Console.ReadLine();
                string[] c = s.Split(' ');
                double sumsred = 0;
                for (int i = 0; i < amount; i++)
                {
                    if ((c.Length < amount) || (c.Length > amount))
                    {
                        Console.WriteLine($"Длина массива не может быть меньше или больше {amount} ");
                        break;
                    }
                    bool norm = double.TryParse(c[i], out a[i]);
                    if (!norm)
                    {
                        Console.WriteLine("Вы ввели что-то не так");
                    }
                    sumsred += a[i];
                }


                double sred = sumsred / amount;

                double proverka = 1;
                int posit = 0;
                for (int i = 0; i < amount; i++)
                {
                    if (Math.Abs(1 - a[i] / sred) < proverka)
                    {
                        posit = i;
                        proverka = Math.Abs(1 - a[i] / sred);
                    }
                }

                Insert(ref a, P, posit + 1);
                Console.Write("Массив с добавлением: [");
                for (int i = 0; i < a.Length; i++)
                {
                    Console.Write($"{a[i]} ");
                }
                Console.Write("]");
            }
            #endregion
            #region Task9_L2
            static void L2N9()
            {
                int ammount;
                Console.WriteLine("Введите кол-во элементов будущего массива");
                bool normul = int.TryParse(Console.ReadLine(), out ammount);
                if (ammount < 2)
                {
                    Console.WriteLine("Слишком мало");
                    return;
                }
                if (!normul)
                {
                    Console.WriteLine("Вы ввели что-то не то");
                    return;
                }
                double[] a = new double[ammount];
                Console.WriteLine($"Введите {ammount} значений/я массива через пробел ");
                string s = Console.ReadLine();
                string[] c = s.Split(' ');
                double max = double.MinValue;
                double min = double.MaxValue;
                int posMax = 0;
                int posMin = 0;

                for (int i = 0; i < ammount; i++)
                {
                    if ((c.Length < ammount) || (c.Length > ammount))
                    {
                        Console.WriteLine($"Длина массива не может быть меньше или больше {ammount}");
                        break;
                    }
                    bool norm = double.TryParse(c[i], out a[i]);
                    if (!norm)
                    {
                        Console.WriteLine("Вы ввели что-то не так");
                    }
                    if (a[i] > max)
                    {
                        max = a[i];
                        posMax = i;
                    }
                    if (a[i] < min)
                    {
                        min = a[i];
                        posMin = i;
                    }
                }

                int counter = 0;

                if (posMax < posMin)
                {
                    (a[posMax], a[posMin]) = (a[posMin], a[posMax]);
                }

                double sum = 0;
                int count = 0;
                for (int i = posMin + 1; i < posMax; i++)
                {
                    sum += a[i];
                    count += 1;
                }
                double sred = sum / count;

                Console.WriteLine(sred);
            }
            #endregion
            #region Task10_L2
            static void L2N10()
            {

                static void Insert(ref double[] arr, int index)
                {
                    double[] newArray = new double[arr.Length - 1];
                    /*                                                
                   for (int i = 0; i < index; i++)
                   {
                       newArray[i] = arr[i];
                   }
                   for (int i = index + 1; i < arr.Length; i++)
                   {
                       newArray[i - 1] = arr[i];
                   }
                   */
                    for (int i = 0; i < newArray.Length; i++)
                    {
                        if (i < index)
                        {
                            newArray[i] = arr[i];
                        }
                        if (i >= index)
                        {
                            newArray[i] = arr[i + 1];
                        }
                    }


                    arr = newArray;
                }
                
                

                int ammount;
                Console.WriteLine("Введите кол-во элементов будущего массива");
                bool normul = int.TryParse(Console.ReadLine(), out ammount);
                if (ammount < 2)
                {
                    Console.WriteLine("Слишком мало");
                    return;
                }
                if (!normul)
                {
                    Console.WriteLine("Вы ввели что-то не то");
                    return;
                }
                double[] a = new double[ammount];
                Console.WriteLine($"Введите {ammount} значений/я массива через пробел ");
                string s = Console.ReadLine();
                string[] c = s.Split(' ');
                int posit = 0;
                double minznach = double.MaxValue;
                for (int i = 0; i < ammount; i++)
                {
                    if ((c.Length < ammount) || (c.Length > ammount))
                    {
                        Console.WriteLine($"Длина массива не может быть меньше или больше {ammount} ");
                        break;
                    }
                    bool norm = double.TryParse(c[i], out a[i]);
                    if (!norm)
                    {
                        Console.WriteLine("Вы ввели что-то не так");
                    }
                    if ((a[i] > 0) & (a[i] < minznach))
                    {
                        minznach = a[i];
                        posit = i;
                    }
                }


                Insert(ref a, posit);
                Console.Write("Массив с удалением: [");
                for (int i = 0; i < a.Length; i++)
                {
                    Console.Write($"{a[i]} ");
                }
                Console.Write("]");
            }
