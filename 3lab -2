            #region Task13_L1
            static void L1N13()
            {
                double[] a = new double[10];
                Console.WriteLine("Введите 10 чисел через пробел");
                string s = Console.ReadLine();
                string[] c = s.Split(' ');
                double[] arr1 = new double[5];
                double[] arr2 = new double[5];
                int countikArr1 = 0;
                int countikArr2 = 0;

                for (int i = 0; i < 10; i++)
                {
                    if ((c.Length < 10) || (c.Length > 10))
                    {
                        Console.WriteLine("Длина массива не может быть меньше или больше 10");
                        break;
                    }
                    bool norm = double.TryParse(c[i], out a[i]);
                    if (!norm)
                    {
                        Console.WriteLine("Вы ввели что-то не так");
                    }
                    if (i % 2 == 0)
                    {
                        arr1[countikArr1] = a[i];
                        countikArr1 += 1;
                    }
                    else
                    {
                        arr2[countikArr2] = a[i];
                        countikArr2 += 1;
                    }
                }
                Console.Write("1й массив: ");
                for (int i = 0; i < 5; i++)
                {
                    Console.Write($"{arr1[i]} ");
                }
                Console.Write("\n2й массив: ");
                for (int i = 0; i < 5; i++)
                {
                    Console.Write($"{arr2[i]} ");
                }

            }
