            #region Alg 11
            static void Alg11()
            {

                int ammount;
                Console.WriteLine("Введите кол-во элементов будущего массива");
                bool normul = int.TryParse(Console.ReadLine(), out ammount);
                if (ammount < 2)
                {
                    Console.WriteLine("Слишком мало");
                    return;
                }
                if (!normul)
                {
                    Console.WriteLine("Вы ввели что-то не то");
                    return;
                }
                double[] a = new double[ammount];
                Console.WriteLine($"Введите {ammount} значений/я массива через пробел ");
                string s = Console.ReadLine();
                string[] c = s.Split(' ');
                for (int i = 0; i < ammount; i++)
                {
                    if ((c.Length < ammount) || (c.Length > ammount))
                    {
                        Console.WriteLine($"Длина массива не может быть меньше или больше {ammount} ");
                        break;
                    }
                    bool norm = double.TryParse(c[i], out a[i]);
                    if (!norm)
                    {
                        Console.WriteLine("Вы ввели что-то не так");
                    }
                }
                Array.Sort(a);
                double x;
                Console.WriteLine("Введите Х");
                bool normulik = double.TryParse(Console.ReadLine(), out x);
                if (!normul)
                {
                    Console.WriteLine("Вы ввели что-то не то");
                    return;
                }
                int ind = 0;
                int ind2 = a.Length - 1;
                int sred;
                int indx = 0;
                while (ind <= ind2)
                {
                    sred = (ind + ind2) / 2;
                    if (a[sred] == x)
                    {
                        indx = sred;
                        break;
                    }
                    else if (a[sred] < x)
                    {
                        ind = sred + 1;
                    }
                    else if (a[sred] > x)
                    {
                        ind2 = sred - 1;
                    }
                }
                Console.WriteLine(indx);
            }
            #endregion
            #region Alg12
            static void Alg12()
            {
             
                int ammount;
                Console.WriteLine("Введите кол-во элементов будущего массива A");
                bool normul = int.TryParse(Console.ReadLine(), out ammount);
                int ammountB;
                int ammountFinal = 0;
                Console.WriteLine("Введите кол-во элементов будущего массива B");
                bool normulB = int.TryParse(Console.ReadLine(), out ammountB);
                if ((ammount < 2)||(ammountB < 2))
                {
                    Console.WriteLine("Слишком мало");
                    return;
                }
                if (!normulB || !normul)
                {
                    Console.WriteLine("Вы ввели что-то не то");
                    return;
                }
                int much = 0;
                ammountFinal = ammount;
                much = Math.Abs(ammount - ammountB);
                if (ammount >= ammountB)
                {
                    ammountFinal = ammountB;
                }


                double[] a = new double[ammount];
                Console.WriteLine($"Введите {ammount} значений/я массива через пробел для массива А");
                string s = Console.ReadLine();
                string[] c = s.Split(' ');
                for (int i = 0; i < ammount; i++)
                {
                    if ((c.Length < ammount) || (c.Length > ammount))
                    {
                        Console.WriteLine($"Длина массива не может быть меньше или больше {ammount} ");
                        break;
                    }
                    bool norm = double.TryParse(c[i], out a[i]);
                    if (!norm)
                    {
                        Console.WriteLine("Вы ввели что-то не так");
                    }
                }
                
                
                double[] b = new double[ammountB];
                Console.WriteLine($"Введите {ammountB} значений/я массива через пробел для массива B ");
                string sB = Console.ReadLine();
                string[] cB = sB.Split(' ');
                for (int i = 0; i < ammountB; i++)
                {
                    if ((cB.Length < ammountB) || (cB.Length > ammountB))
                    {
                        Console.WriteLine($"Длина массива не может быть меньше или больше {ammountB} ");
                        break;
                    }
                    bool norm = double.TryParse(cB[i], out b[i]);
                    if (!norm)
                    {
                        Console.WriteLine("Вы ввели что-то не так");
                    }
                }
                

                double[] massik = new double[ammount + ammountB];
                int countik = 0;
                for (int i = 0; i < ammountFinal ; i++)
                {
                    massik[2 * i] = a[countik];
                    massik[2 * i + 1] = b[countik];
                    countik += 1;

                }
                
                for (int i = ammountFinal * 2; i < ammountFinal * 2 + much; i++)
                {
                    if (ammount >= ammountB)
                    {
                        massik[i] = a[countik];
                        countik += 1;
                    }
                    if (ammount < ammountB)
                    {
                        massik[i] = b[countik];
                        countik += 1;
                    }
                }
                
                for (int i = 0; i < massik.Length; i++)
                {
                    Console.Write($"{massik[i]} ");
                }

                
            }
            #endregion
            #region Alg13
            static void Alg13()
            {

                int ammount;
                Console.WriteLine("Введите кол-во элементов будущего массива A");
                bool normul = int.TryParse(Console.ReadLine(), out ammount);
                int ammountB;
                int ammountFinal = 0;
                Console.WriteLine("Введите кол-во элементов будущего массива B");
                bool normulB = int.TryParse(Console.ReadLine(), out ammountB);
                if ((ammount < 2) || (ammountB < 2))
                {
                    Console.WriteLine("Слишком мало");
                    return;
                }
                if (!normulB || !normul)
                {
                    Console.WriteLine("Вы ввели что-то не то");
                    return;
                }

                ammountFinal = ammount + ammountB;
                


                double[] a = new double[ammount];
                Console.WriteLine($"Введите {ammount} значений/я массива через пробел для массива А");
                string s = Console.ReadLine();
                string[] c = s.Split(' ');
                for (int i = 0; i < ammount; i++)
                {
                    if ((c.Length < ammount) || (c.Length > ammount))
                    {
                        Console.WriteLine($"Длина массива не может быть меньше или больше {ammount} ");
                        break;
                    }
                    bool norm = double.TryParse(c[i], out a[i]);
                    if (!norm)
                    {
                        Console.WriteLine("Вы ввели что-то не так");
                    }
                }


                double[] b = new double[ammountB];
                Console.WriteLine($"Введите {ammountB} значений/я массива через пробел для массива B ");
                string sB = Console.ReadLine();
                string[] cB = sB.Split(' ');
                for (int i = 0; i < ammountB; i++)
                {
                    if ((cB.Length < ammountB) || (cB.Length > ammountB))
                    {
                        Console.WriteLine($"Длина массива не может быть меньше или больше {ammountB} ");
                        break;
                    }
                    bool norm = double.TryParse(cB[i], out b[i]);
                    if (!norm)
                    {
                        Console.WriteLine("Вы ввели что-то не так");
                    }
                }
                Array.Sort(a);
                Array.Sort(b);
                Array.Reverse(a);
                Array.Reverse(b);
                double[] massik = new double[ammountFinal];
                int countA = 0;
                int countB = 0;
                int op = 0;
                
                while ((countA < a.Length)||(countB < b.Length))
                {
                    
                    if (countA == a.Length)
                    {
                        massik[op] = b[countB];
                        countB += 1;
                        op += 1;
                        continue;
                    }
                    if (countB == a.Length)
                    {
                        massik[op] = a[countA];
                        countA += 1;
                        op += 1;
                        continue;
                    }
                    if (a[countA] >= b[countB])
                    {
                        massik[op] = a[countA];
                        countA += 1;
                        op += 1;
                    }
                    else
                    {
                        massik[op] = b[countB];
                        countB += 1;
                        op += 1;
                    }

                }
               
                for (int i = 0; i < massik.Length; i++)
                {
                    Console.Write($"{massik[i]} ");
                }


            }
            #endregion
            #region Alg 14
            static void Alg14()
            {

                int ammount;
                Console.WriteLine("Введите кол-во элементов будущего массива");
                bool normul = int.TryParse(Console.ReadLine(), out ammount);
                if (ammount < 2)
                {
                    Console.WriteLine("Слишком мало");
                    return;
                }
                if (!normul)
                {
                    Console.WriteLine("Вы ввели что-то не то");
                    return;
                }
                double[] a = new double[ammount];
                Console.WriteLine($"Введите {ammount} значений/я массива через пробел ");
                string s = Console.ReadLine();
                string[] c = s.Split(' ');
                for (int i = 0; i < ammount; i++)
                {
                    if ((c.Length < ammount) || (c.Length > ammount))
                    {
                        Console.WriteLine($"Длина массива не может быть меньше или больше {ammount} ");
                        break;
                    }
                    bool norm = double.TryParse(c[i], out a[i]);
                    if (!norm)
                    {
                        Console.WriteLine("Вы ввели что-то не так");
                    }
                }
                double l;
                int back = a.Length - 1;
                for (int i = 0; i < a.Length / 2 ; i++)
                {
                    
                    l = a[i];
                    a[i] = a[back];
                    a[back] = l;
                    back--;
                }
                for (int i = 0; i < a.Length; i++)
                {
                    Console.Write($"{a[i]} ");
                }
                
                
            }
