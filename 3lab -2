            #region Task15_L2
            static void L2N15()
            {

                static void Insert(ref double[] arr, ref double[] arr2, int index)
                {
                    double[] newArray = new double[arr.Length + arr2.Length];


                    for (int i = 0; i < index; i++)
                    {
                        newArray[i] = arr[i];
                    }
                    int k = 0;
                    for (int i = index; i <= arr2.Length + 1; i++)
                    {
                        newArray[i] = arr2[k];
                        k += 1;
                    }
                    for (int i = index; i < arr.Length; i++)
                    {
                        newArray[i + arr2.Length] = arr[i];
                    }
                    arr = newArray;
                }

                #region massA
                int ammount;
                Console.WriteLine("Введите кол-во элементов будущего массива A");
                bool normul = int.TryParse(Console.ReadLine(), out ammount);
                if (ammount < 2)
                {
                    Console.WriteLine("Слишком мало");
                    return;
                }
                if (!normul)
                {
                    Console.WriteLine("Вы ввели что-то не то");
                    return;
                }
                double[] a = new double[ammount];
                Console.WriteLine($"Введите {ammount} значений/я массива через пробел ");
                string s = Console.ReadLine();
                string[] c = s.Split(' ');
                for (int i = 0; i < ammount; i++)
                {
                    if ((c.Length < ammount) || (c.Length > ammount))
                    {
                        Console.WriteLine($"Длина массива не может быть меньше или больше {ammount} ");
                        break;
                    }
                    bool norm = double.TryParse(c[i], out a[i]);
                    if (!norm)
                    {
                        Console.WriteLine("Вы ввели что-то не так");
                    }
                }
                #endregion
                #region ArrB
                int ammountB;
                Console.WriteLine("Введите кол-во элементов будущего массива B");
                bool normulB = int.TryParse(Console.ReadLine(), out ammountB);
                if (ammountB < 2)
                {
                    Console.WriteLine("Слишком мало");
                    return;
                }
                if (!normulB)
                {
                    Console.WriteLine("Вы ввели что-то не то");
                    return;
                }
                double[] b = new double[ammountB];
                Console.WriteLine($"Введите {ammountB} значений/я массива через пробел ");
                string sB = Console.ReadLine();
                string[] cB = sB.Split(' ');
                for (int i = 0; i < ammountB; i++)
                {
                    if ((cB.Length < ammountB) || (cB.Length > ammountB))
                    {
                        Console.WriteLine($"Длина массива не может быть меньше или больше {ammountB} ");
                        break;
                    }
                    bool norm = double.TryParse(cB[i], out b[i]);
                    if (!norm)
                    {
                        Console.WriteLine("Вы ввели что-то не так");
                    }
                }
                #endregion
                int K;
                Console.WriteLine("Введите K");
                bool normulik = int.TryParse(Console.ReadLine(), out K);
                if ((!normulik) || (K >= a.Length))
                {
                    Console.WriteLine("Вы ввели что-то не то");
                    return;
                }
                int posit = default;
                int countik = 0;
                for (int i = a.Length - 1; i >= 0; i--)
                {
                    if (a[i] > 0)
                    {
                        posit = i;
                        countik += 1;
                        break;
                    }
                }


                Insert(ref a, ref b, K + 1);
                Console.Write("Массив с добавлением: [");
                for (int i = 0; i < a.Length; i++)
                {
                    Console.Write($"{a[i]} ");
                }
                Console.Write("]");
            }
